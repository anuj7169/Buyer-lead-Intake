datasource db {
  provider = "postgresql" // or "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Lead {
  id          Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  phone       String?
  notes       String?
  createdAt   DateTime @default(now())
}
